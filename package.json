{
  "name": "newton",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "ios-simulator": "expo start --ios --dev-client",
    "clear": "expo start --clear",
    "tunnel": "expo start --tunnel",
    "reset": "./scripts/reset-dev.sh",
    "clean": "rm -rf .expo && rm -rf node_modules/.cache && npm cache clean --force",
    "start-safe": "npx expo start --port 8082 --clear",
    "dev": "npm run reset && npm run start-safe",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint-check": "npm run lint -- --max-warnings 5",
    "lint-fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "format-check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "type-check": "tsc --noEmit",
    "pre-build": "npm run lint-check && npm run type-check",
    "build": "npm run pre-build && expo build",
    "security-audit": "npm audit --audit-level moderate",
    "analyze-bundle": "npx expo-bundle-analyzer"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@supabase/mcp-server-supabase": "^0.4.5",
    "@supabase/supabase-js": "^2.52.0",
    "base64-arraybuffer": "^1.0.2",
    "expo": "~53.0.13",
    "expo-dev-client": "~5.2.2",
    "expo-file-system": "^18.1.11",
    "expo-image-picker": "~16.1.4",
    "expo-status-bar": "~2.2.3",
    "pg": "^8.16.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.4",
    "react-native-dotenv": "^3.4.11",
    "react-native-markdown-display": "^7.0.2",
    "react-native-safe-area-context": "5.4.0",
    "react-native-svg": "^15.12.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "^0.20.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "latest",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  },
  "private": true
}
