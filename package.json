  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "ios-simulator": "expo start --ios --dev-client",
    "clear": "expo start --clear",
